{
  "name": "qys-pdf-viewer",
  "version": "0.1.40",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:cli": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "cross-env TS_NODE_PROJECT='build/tsconfig.node.json' ENV='development' webpack-dev-server --config build/webpack/webpack.dev.config.ts --progress",
    "build": "cross-env TS_NODE_PROJECT='build/tsconfig.node.json' ENV='production' webpack --config build/webpack/webpack.prod.config.ts"
  },
  "dependencies": {
    "intersection-observer": "^0.7.0",
    "lodash": "^4.17.15",
    "qys-pdf": "^1.0.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@types/friendly-errors-webpack-plugin": "^0.1.2",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.32.1",
    "@types/webpack-bundle-analyzer": "^2.13.2",
    "@types/webpack-dev-server": "^3.1.7",
    "@types/webpack-merge": "^4.1.5",
    "@vue/babel-preset-jsx": "^1.1.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "style-loader": "^1.0.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-merge": "^4.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "description": "## Project setup ``` yarn install ```",
  "main": "dist/pdf-viewer.js",
  "author": "Dreamcatcher",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StartCareerWithDream/qys-pdf-viewer.git"
  },
  "keywords": [
    "pdfjs",
    "pdfjs-dist",
    "vue-pdf",
    "qys-pdf"
  ],
  "bugs": {
    "url": "https://github.com/StartCareerWithDream/qys-pdf-viewer/issues"
  },
  "homepage": "https://github.com/StartCareerWithDream/qys-pdf-viewer#readme"
